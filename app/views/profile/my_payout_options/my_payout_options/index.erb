<div class='row front_content_main_row my_payout_options_list_main_row'>
  <div class="w500_centerd" id="content">
    <div class="row front_page_content_row">
      <div class="col-12">
        <div class="row front_page_title_row hidden_in_modal">
          <div class="col-12">
            <h3>My payout options</h3><br/>
            <%= link_to 'Add new payout option', :new_profile_my_payout_option, class: 'btn btn-sm btn-outline-success' %>
            <br/><br/>
          </div>
        </div>
        <div class="row front_page_content_row">
          <div class="col-12">
            <div class="row front_payout_options_list_row">
              <% @items.each do |item| %>
                <div class="col-12 front_payout_option_list_item">
                  <%= render(:partial => 'user_profile_my_payout_options_list_item', locals: { item: item }) %>
                </div>
              <% end %>
            </div>
          </div>
        </div>
        <div class="row hidden_in_modal">
          <div class="col-12">
            <br /><br /><br /><br /><br /><br />
            <%= link_to "Back to profile page", user_root_path %>
            <br/><br/><br/><br/><br/>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
